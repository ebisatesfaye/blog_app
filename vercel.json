{
    "builds": [{
        "src": "django_blog/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" },
        "build": {
          "env": {
            "PG_CONFIG": "/usr/local/bin/pg_config"
          }
        } 
    }],
    "routes": [
        {
            "src": "/blog/static/blog/(.*)",
            "dest": "/static/$1"
          },
          {
            "src": "(.*)",
            "dest": "django_blog/wsgi.py"
          }
    ]
}

